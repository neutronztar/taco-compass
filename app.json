{
    "expo": {
        "scheme": "acme",
        "web": {
            "bundler": "metro"
        },
        "plugins": [
            "expo-router",
            [
                "expo-location",
                {
                    "locationWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
                }
            ],
            [
                "expo-screen-orientation",
                {
                    "initialOrientation": "PORTRAIT_UP"
                }
            ]
        ],
        "name": "taco-compass",
        "slug": "taco-compass"
    }
}
